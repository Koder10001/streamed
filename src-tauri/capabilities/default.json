{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": ["https://*", "http://*"]
  },
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*"
        },
        {
          "url": "https://*"
        }
      ]
    },
    "http:default"
  ]
}