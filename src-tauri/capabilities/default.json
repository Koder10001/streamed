{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": ["tauri://*","http://tauri.localhost/*","https://streami.su/", "https://streamed.pk/", "https://streamed.st/", "https://embedsports.top/", "webkit-pdfjs-viewer://*"]
  },
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "tauri://*"
        },
        {
          "url": "http://tauri.localhost/"
        },
        {
          "url": "https://streami.su/"
        },
        {
          "url": "https://streamed.pk/"
        },
        {
          "url": "https://streamed.st/"
        }

      ]
    }
  ]
}