{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Streamed",
  "version": "0.1.0",
  "identifier": "com.kod.streamedpk",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "streamedpk",
        "width": 1920,
        "height": 1080
      }
    ],
    "security": {
      "csp": "default-src http://tauri.localhost/ 'self' 'unsafe-hashes' 'unsafe-inline' https://streami.su/ https://streamed.pk/ https://streamed.st/; connect-src: ipc: http://ipc.localhost/ ; frame-src https://embedsports.top/;",
      "dangerousDisableAssetCspModification": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
